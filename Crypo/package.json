{
  "name": "crypo",
  "version": "1.0.0",
  "description": "A modular UI kit for blockchain exchange dashboard",
  "main": "index.js",
  "scripts": {
    "start": "concurrently --names \"SASS,WATCH,SERVE\" -c \"bgBlue.bold,bgMagenta.bold,bgGreen.bold\" \"npm run sass\" \"npm run serve\"",
