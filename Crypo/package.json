    "sass": "sass --watch assets/scss/style.scss:assets/css/style.css"
  },
  "keywords": [],
  "author": "Trendy Ui",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^5.2.0",
    "lite-server": "^2.5.4",
    "sass": "^1.50.0"
  }
}
